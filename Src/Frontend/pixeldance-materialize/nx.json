{
	"npmScope": "pixeldance-materialize",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"projects": {
		"core-lib": {
			"tags": []
		},
		"identity-lib": {
			"tags": []
		},
		"recipe-app": {
			"tags": []
		},
		"recipe-app-e2e": {
			"tags": [],
			"implicitDependencies": ["recipe-app"]
		},
		"recipe-lib": {
			"tags": []
		},
		"ui-lib": {
			"tags": []
		}
	}
}
