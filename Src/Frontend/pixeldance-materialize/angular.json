{
	"version": 1,
	"projects": {
		"core-lib": {
			"projectType": "library",
			"root": "libs/core-lib",
			"sourceRoot": "libs/core-lib/src",
			"prefix": "pxd",
			"architect": {
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/core-lib"],
					"options": {
						"jestConfig": "libs/core-lib/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/core-lib/src/**/*.ts", "libs/core-lib/src/**/*.html"]
					}
				}
			}
		},
		"identity-lib": {
			"projectType": "library",
			"root": "libs/identity-lib",
			"sourceRoot": "libs/identity-lib/src",
			"prefix": "auth",
			"architect": {
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/identity-lib"],
					"options": {
						"jestConfig": "libs/identity-lib/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/identity-lib/src/**/*.ts", "libs/identity-lib/src/**/*.html"]
					}
				}
			}
		},
		"recipe-app": {
			"projectType": "application",
			"root": "apps/recipe-app",
			"sourceRoot": "apps/recipe-app/src",
			"prefix": "pxd",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/recipe-app",
						"index": "apps/recipe-app/src/index.html",
						"main": "apps/recipe-app/src/main.ts",
						"polyfills": "apps/recipe-app/src/polyfills.ts",
						"tsConfig": "apps/recipe-app/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": ["apps/recipe-app/src/favicon.ico", "apps/recipe-app/src/assets"],
						"styles": ["apps/recipe-app/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "1.75mb",
									"maximumError": "2mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/recipe-app/src/environments/environment.ts",
									"with": "apps/recipe-app/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "recipe-app:build:production"
						},
						"development": {
							"browserTarget": "recipe-app:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "recipe-app:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/recipe-app/src/**/*.ts", "apps/recipe-app/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/recipe-app"],
					"options": {
						"jestConfig": "apps/recipe-app/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"recipe-app-e2e": {
			"root": "apps/recipe-app-e2e",
			"sourceRoot": "apps/recipe-app-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/recipe-app-e2e/cypress.json",
						"tsConfig": "apps/recipe-app-e2e/tsconfig.e2e.json",
						"devServerTarget": "recipe-app:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "recipe-app:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/recipe-app-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"recipe-lib": {
			"projectType": "library",
			"root": "libs/recipe-lib",
			"sourceRoot": "libs/recipe-lib/src",
			"prefix": "pxd",
			"architect": {
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/recipe-lib"],
					"options": {
						"jestConfig": "libs/recipe-lib/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/recipe-lib/src/**/*.ts", "libs/recipe-lib/src/**/*.html"]
					}
				}
			}
		},
		"ui-lib": {
			"projectType": "library",
			"root": "libs/ui-lib",
			"sourceRoot": "libs/ui-lib/src",
			"prefix": "ui",
			"architect": {
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/ui-lib"],
					"options": {
						"jestConfig": "libs/ui-lib/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/ui-lib/src/**/*.ts", "libs/ui-lib/src/**/*.html"]
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"schematics": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "recipe-app"
}
