{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Args": { "path": "../../../storage/logs/dev-app-.log" }
      }
    ]
  },
  "AllowedHosts": "*",
  "EfCore": {
    "ConnectionString": "CONNECTION_STRING",
    "UseInMemoryDatabase": false
  }
}